<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN" "http://tiles.apache.org/dtds/tiles-config_2_1.dtd">

<tiles-definitions>

	
    <!-- Default Main Template -->
    <definition name=".mainTemplate" template="/WEB-INF/templates/template.jsp">
        <put-attribute name="header" value="/WEB-INF/templates/header.jsp" />
        <put-attribute name="footer" value="/WEB-INF/templates/footer.jsp" />
        <put-attribute name="left-menu" value="/WEB-INF/templates/left-menu.jsp" />
        <put-attribute name="body" value="" />
    </definition>
	
    <definition name="newUser" extends=".mainTemplate">
        <put-attribute name="body" value="/WEB-INF/views/user/newUser.jsp" />
    </definition>

</tiles-definitions>